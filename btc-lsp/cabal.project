  signable:proto-lens, signable:proto-lens-runtime,
  http2-grpc-proto-lens:proto-lens, http2-client:http2,
  lnd-client:aeson, chronos:aeson, lnd-client:base64-bytestring,
  qrcode-juicypixels:base64-bytestring, http2-client-grpc:http2-client,
  lnd-client:network-bitcoin, http2-client-grpc:http2,
  yesod-colonnade:base, lnd-client:template-haskell,
  lnd-client:bytestring, lnd-client:containers,
  lnd-client:text
